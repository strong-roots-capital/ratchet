{
  "name": "@strong-roots-capital/ratchet",
  "version": "3.0.5",
  "description": "Filter a stream of values monotonically",
  "main": "dist/src/ratchet.js",
  "types": "dist/src/ratchet.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "ava": "ava -s -v",
    "lint": "eslint --ext ts .",
    "lint:fix": "eslint --ext ts --fix .",
    "test": "c8 ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strong-roots-capital/ratchet"
  },
  "keywords": [
    "ratchet",
    "monotonic"
  ],
  "author": "Eric Crosson <eric.s.crosson@utexas.edu> (https://github.com/strong-roots-capital)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/strong-roots-capital/ratchet/issues"
  },
  "homepage": "https://github.com/strong-roots-capital/ratchet",
  "dependencies": {
    "fp-ts": "^2.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.42.0",
    "@typescript-eslint/parser": "5.42.0",
    "ava": "5.0.1",
    "ava-fast-check": "6.0.0",
    "c8": "7.12.0",
    "eslint": "8.26.0",
    "fast-check": "3.3.0",
    "random-record": "2.0.3",
    "semantic-release": "19.0.5",
    "timeseries-record": "1.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "files": [
    "dist/src/"
  ],
  "ava": {
    "files": [
      "!dist/**/*.d.ts",
      "test/**/test-*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
