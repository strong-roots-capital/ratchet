{
  "name": "@strong-roots-capital/ratchet",
  "version": "3.0.8",
  "description": "Filter a stream of values monotonically",
  "main": "dist/src/ratchet.js",
  "types": "dist/src/ratchet.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "ava": "ava -s -v",
    "test": "c8 ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strong-roots-capital/ratchet"
  },
  "keywords": [
    "ratchet",
    "monotonic"
  ],
  "author": "Eric Crosson <eric.s.crosson@utexas.edu> (https://github.com/strong-roots-capital)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/strong-roots-capital/ratchet/issues"
  },
  "homepage": "https://github.com/strong-roots-capital/ratchet",
  "dependencies": {
    "fp-ts": "^2.0.0"
  },
  "devDependencies": {
    "ava": "5.3.0",
    "ava-fast-check": "6.0.0",
    "c8": "8.0.0",
    "fast-check": "3.10.0",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "files": [
    "dist/src/"
  ],
  "ava": {
    "files": [
      "!dist/**/*.d.ts",
      "test/**/test-*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
